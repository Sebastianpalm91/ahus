{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
  ],
     "headers": [ {
       "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
       "headers" : [ {
         "key" : "Access-Control-Allow-Origin",
         "value" : "max-age=86400"
       } ]
       }, {
       "source" : "/Assets/Images/*.@(jpg|jpeg|gif|png)",
       "headers" : [ {
         "key" : "Cache-Control",
         "value" : "max-age=86400"
       } ]
       }, {
       "source" : "404.html",
       "headers" : [ {
         "key" : "Cache-Control",
         "value" : "max-age=400"
       } ]
     } ],
     "cleanUrls": true,
     "trailingSlash": false
  }
}
